{
    "name": "zjkal/twig2html-core",
    "description": "Core library for converting Twig templates to static HTML files",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "zjkal\\twig2html\\core\\": "src/"
        }
    },
    "authors": [
        {
            "name": "zjkal",
            "email": "zjkal@qq.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "twig/twig": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}